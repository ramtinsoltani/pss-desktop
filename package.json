{
  "name": "pss-desktop",
  "description": "Personal Shared Storage Client",
  "version": "0.0.0",
  "homepage": "https://github.com/ramtinsoltani/pss-desktop",
  "author": {
    "name": "Ramtin Soltani",
    "email": "ramtinsoltani93@gmail.com"
  },
  "license": "UNLICENSED",
  "repository": {
    "url": "https://github.com/ramtinsoltani/pss-desktop"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "electron": "npx electron ./dist/main.js",
    "clean": "rm -rf ./dist && rm -rf ./build-linux && rm -rf ./build-win",
    "build-web": "ng build --prod --base-href ./ && npm run main-compile && npm run main-compress",
    "main-compile": "tsc main.ts --lib 'es2018,dom' --target es5 --esModuleInterop --resolveJsonModule --outDir ./dist",
    "main-compress": "npx terser --compress --mangle --toplevel --output ./dist/main.js -- ./dist/main.js",
    "build-linux": "node build build-linux",
    "build-win": "node build build-win",
    "build-linux-clean": "npm run clean && npm run build-web && npm run build-linux",
    "build-win-clean": "npm run clean && npm run build-web && npm run build-win"
  },
  "main": "main.js",
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.2.0",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/forms": "~7.2.0",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/router": "~7.2.0",
    "@fortawesome/angular-fontawesome": "^0.3.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.18",
    "@fortawesome/free-solid-svg-icons": "^5.8.2",
    "core-js": "^2.5.4",
    "fs-extra": "^8.0.1",
    "lodash": "^4.17.11",
    "ngx-electron": "^2.1.1",
    "request": "^2.88.0",
    "rxjs": "~6.3.3",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular/cli": "~7.3.9",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@types/fs-extra": "^7.0.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/lodash": "^4.14.130",
    "@types/node": "^12.0.2",
    "@types/request": "^2.48.1",
    "codelyzer": "~4.5.0",
    "electron": "^5.0.1",
    "electron-builder": "^20.40.2",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.0",
    "terser": "^4.0.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.2.2"
  }
}
